<h1>検索ページ</h1>
<%= form_with url: chanels_search_path, local: true, method: :get do |f| %>
    <div id="form-area" class="mb-3">
        <%= f.select :tag, @tags.map{|tag|[tag.name]} , include_blank: "選択して下さい",class:"select" %>
    </div>
    <%= f.submit "検索", class: "btn btn-primary" %>
 <% end %>

 

 <%= javascript_pack_tag 'chanels/index' %>